package methods;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class MethodsWebdriver {

	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\Pranav\\Documents\\Eclipse_Software_Testing\\Automation\\Drivers\\chromedriver.exe");
		
		//Open the Browser.
		WebDriver driver = new ChromeDriver();
		
		driver.get("https://1.bp.blogspot.com/-opzN0hn_YLM/XSm21pjMBkI/AAAAAAAAm4I/wq37SU4LYy0A9pZFkS_8jOeiyREMzB3JwCLcBGAs/s1600/1454440161-aslan-lion-file.jpg");
    
  //set the position of window.
		Point p = new Point(600, 0);
    driver.manage().window().setPosition(p);
   }
  }
