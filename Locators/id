package basics;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Locators {

	public static void main(String[] args) {
		System.setProperty("webdriver.chrome.driver", "C:\\Users\\Pranav\\Documents\\Eclipse_Software_Testing\\Automation\\Drivers\\chromedriver.exe");

		// Launch Browser.
		WebDriver driver = new ChromeDriver();
		
		// Launch URL
		driver.get("https://account.booking.com/sign-in?op_-LXS4vjrJToAQgBY-ufOmQaAAQE"); 
		
    // Locating webelement using id
    driver.findElement(By.id("username")).sendKeys("xyz");
    }
   }
